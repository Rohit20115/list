    "#find the reverse of a number provided by the user (any number of digit)\n",
    "\n",
    "n = int(input(\"enter the number\"))\n",
    "\n",
    "digit_list = list(map(int,str(n)))\n",
    "\n",
    "digit_list = digit_list[::-1]\n",
    "\n",
    "num = int(\"\".join(map(str,digit_list)))\n",
    "\n",
    "print(num)"
   ]
